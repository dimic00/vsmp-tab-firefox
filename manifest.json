{
	"manifest_version": 2,
	"name": "VSMP Tab",
	"version": "1.1",
	"description": "Very slow movie played in browser tab.",
	"icons": {
		"48": "icons/48.png",
		"96": "icons/96.png"
	},
	"permissions": ["tabs", "webRequest"],
	"chrome_settings_overrides": {
		"homepage": "index.html"
	},
	"chrome_url_overrides": {
		"newtab": "index.html"
	}
}
